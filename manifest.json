{
  "manifest_version": 3,
  "name": "Pinned Tabs Restorer",
  "version": "1.1.3",
  "description": "Automatically saves and restores your pinned tabs.",
  "permissions": [
    "tabs",
    "storage",
    "windows"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}